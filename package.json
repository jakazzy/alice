{
  "name": "alicerx",
  "version": "1.0.0",
  "description": "A simple facebook chatbot for assisting in triaging patients (only mild to moderate this excludes severe patients since at the stage a chatbot is of no use) for treatment of CV19 ",
  "main": "app.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "node_modules/.bin/prettier . \"src/**/*.{js,json}\"",
      "format": "npm run prettier -- --write",
      "lint": "node_modules/.bin/eslint \"src/**/*.{js,jsx,css}\"",
      "lint:fix": "npm run lint -- --fix"
  }, "lint-staged": {
    "src/**/*.{js,jsx}": [
        "npm run format",
        "npm run lint:fix"
    ]
},
"husky": {
    "hooks": {
        "pre-commit": "lint-staged"
    }
},
"type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jakazzy/alice.git"
  },
  "keywords": [
    "facebook",
    "chatbot",
    "dialogflow",
    "express",
    "facebook",
    "messenger",
    "CV-19"
  ],
  "author": "jakazzy",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jakazzy/alice/issues"
  },
  "homepage": "https://github.com/jakazzy/alice#readme",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dialogflow": "^1.2.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "request": "^2.88.2",
    "uuid": "^8.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/node": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "babel-preset-airbnb": "^5.0.0",
    "eslint": "^7.3.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.21.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
